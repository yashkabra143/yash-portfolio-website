{
    "version": 2,
    "builds": [
      {
        "src": "dist/server/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "dist/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "dist/server/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "dist/$1"
      }
    ]
  }